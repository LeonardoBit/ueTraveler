<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- This ensures that the app recognizes NDEF-formatted NFC tags -->
    <tech-list>
        <tech>android.nfc.tech.Ndef</tech>
    </tech-list>

    <!-- This allows detection of raw, unformatted NFC tags -->
    <tech-list>
        <tech>android.nfc.tech.NdefFormatable</tech>
    </tech-list>

    <!-- If your NFC tag uses MIFARE Classic -->
    <tech-list>
        <tech>android.nfc.tech.MifareClassic</tech>
    </tech-list>

    <!-- If your NFC tag uses MIFARE Ultralight -->
    <tech-list>
        <tech>android.nfc.tech.MifareUltralight</tech>
    </tech-list>

    <!-- If your NFC tag supports ISO-Dep (e.g., contactless credit cards) -->
    <tech-list>
        <tech>android.nfc.tech.IsoDep</tech>
    </tech-list>

    <!-- If your NFC tag supports NFC-A (ISO 14443-3A) -->
    <tech-list>
        <tech>android.nfc.tech.NfcA</tech>
    </tech-list>

    <!-- If your NFC tag supports NFC-B (ISO 14443-3B) -->
    <tech-list>
        <tech>android.nfc.tech.NfcB</tech>
    </tech-list>

    <!-- If your NFC tag supports NFC-F (FeliCa, Japan transit cards) -->
    <tech-list>
        <tech>android.nfc.tech.NfcF</tech>
    </tech-list>

    <!-- If your NFC tag supports NFC-V (ISO 15693, often used in RFID) -->
    <tech-list>
        <tech>android.nfc.tech.NfcV</tech>
    </tech-list>

</resources>